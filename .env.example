# AutoCloudFormer Environment Variables

# Application Settings
APP_NAME=AutoCloudFormer
APP_ENV=development
APP_DEBUG=True

# TensorFlow Settings
TF_MODEL_PATH=/path/to/model
TF_BATCH_SIZE=32
TF_EPOCHS=10
TF_LEARNING_RATE=0.001

# AWS Settings
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY_ID
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_ACCESS_KEY
AWS_REGION=us-west-2
AWS_BUCKET_NAME=auto-cloud-former-bucket

# Google Cloud Settings
GOOGLE_CLOUD_PROJECT_ID=YOUR_GOOGLE_CLOUD_PROJECT_ID
GOOGLE_CLOUD_SERVICE_ACCOUNT_KEY_FILE=/path/to/service_account_key.json
GOOGLE_CLOUD_STORAGE_BUCKET_NAME=auto-cloud-former-bucket

# Database Settings
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=your_username
DB_PASSWORD=your_password
DB_NAME=auto_cloud_former

# Security Settings
SECRET_KEY=your_secret_key
JWT_SECRET_KEY=your_jwt_secret_key

# Logging Settings
LOG_LEVEL=DEBUG
LOG_FILE=/path/to/log/file.log

# Other Settings
DATA_DIR=/path/to/data/directory
MODEL_DIR=/path/to/model/directory
OUTPUT_DIR=/path/to/output/directory
